{
  "kind": "collectionType",
  "collectionName": "tickets",
  "info": {
    "name": "Ticket",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "subcategory": {
      "via": "tickets",
      "model": "subcategory"
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "tickets"
    },
    "requirements": {
      "type": "json"
    },
    "commune": {
      "model": "commune",
      "via": "tickets"
    },
    "description": {
      "type": "richtext"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "Publicado",
        "Propuesta",
        "Contrato",
        "Completado"
      ],
      "default": "Publicado",
      "required": true
    },
    "proposals": {
      "via": "ticket",
      "collection": "proposal"
    }
  }
}
