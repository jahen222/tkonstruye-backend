{
  "kind": "collectionType",
  "collectionName": "regions",
  "info": {
    "name": "Region"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "maxLength": 32
    },
    "country": {
      "model": "country",
      "via": "regions"
    },
    "provinces": {
      "via": "region",
      "collection": "province"
    }
  }
}
